@model IEnumerable<Overgear.Models.Boot>

@{
    ViewData["Title"] = "Inventory";
}
<h2>@ViewData["Title"]</h2>

@{
    IEnumerable<Boot> boots = ViewData["Boots"] as IEnumerable<Boot>;
}

<table class="table">
    <h3>Boots</h3>
    <tr>
        <th scope="col">Description</th>
        <th scope="col">Colour</th>
        <th scope="col">Size</th>
        <th scope="col">Quantity</th>
    </tr>

    @foreach (Boot boot in boots)
    {
        <tr>
            <td>@boot.Description</td>
            <td>@boot.Colour</td>
            <td>@boot.Size</td>
            <td>@boot.Quantity</td>
        </tr>
    }
</table> 
